S_000000000279b3a0 .scope module, "testbench" "testbench" 2 4;
 .timescale -9 -12;
v00000000027627b0_0 .var "CLK", 0 0;
v0000000002762850_0 .var "count", 3 0;
v00000000027628f0_0 .net "out", 3 0, v0000000002799850_0;  1 drivers
E_0000000002799040 .event posedge, v00000000027627b0_0;
L_0000000002762990 .part v0000000002762850_0, 0, 3;
S_0000000002798500 .scope module, "l6" "lab6" 2 9, 3 1 0, S_000000000279b3a0;
 .timescale -9 -12;
    .port_info 0 /INPUT 3 "in"
    .port_info 1 /OUTPUT 4 "out"
v0000000002798680_0 .net "in", 2 0, L_0000000002762990;  1 drivers
v0000000002799850_0 .var "out", 3 0;
E_0000000002798b80 .event edge, v0000000002798680_0;
    .scope S_0000000002798500;
T_0 ;
    %wait E_0000000002798b80;
    %load/vec4 v0000000002798680_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 3;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 3;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %dup/vec4;
