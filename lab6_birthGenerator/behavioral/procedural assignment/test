    %cmp/u;
    %jmp/1 T_0.3, 6;
    %dup/vec4;
    %pushi/vec4 4, 0, 3;
    %cmp/u;
    %jmp/1 T_0.4, 6;
    %dup/vec4;
    %pushi/vec4 5, 0, 3;
    %cmp/u;
    %jmp/1 T_0.5, 6;
    %dup/vec4;
    %pushi/vec4 6, 0, 3;
    %cmp/u;
    %jmp/1 T_0.6, 6;
    %dup/vec4;
    %pushi/vec4 7, 0, 3;
    %cmp/u;
    %jmp/1 T_0.7, 6;
    %vpi_call 3 18 "$display", "Error in input" {0 0 0};
    %jmp T_0.9;
T_0.0 ;
    %pushi/vec4 1, 0, 4;
    %store/vec4 v0000000002799850_0, 0, 4;
    %jmp T_0.9;
T_0.1 ;
    %pushi/vec4 9, 0, 4;
    %store/vec4 v0000000002799850_0, 0, 4;
    %jmp T_0.9;
T_0.2 ;
    %pushi/vec4 9, 0, 4;
    %store/vec4 v0000000002799850_0, 0, 4;
    %jmp T_0.9;
T_0.3 ;
    %pushi/vec4 7, 0, 4;
    %store/vec4 v0000000002799850_0, 0, 4;
    %jmp T_0.9;
T_0.4 ;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0000000002799850_0, 0, 4;
    %jmp T_0.9;
T_0.5 ;
    %pushi/vec4 7, 0, 4;
    %store/vec4 v0000000002799850_0, 0, 4;
    %jmp T_0.9;
T_0.6 ;
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0000000002799850_0, 0, 4;
    %jmp T_0.9;
T_0.7 ;
    %pushi/vec4 8, 0, 4;
    %store/vec4 v0000000002799850_0, 0, 4;
    %jmp T_0.9;
T_0.9 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_000000000279b3a0;
T_1 ;
    %delay 10000, 0;
    %load/vec4 v00000000027627b0_0;
    %inv;
    %store/vec4 v00000000027627b0_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_000000000279b3a0;
T_2 ;
    %wait E_0000000002799040;
    %load/vec4 v0000000002762850_0;
    %addi 1, 0, 4;
    %store/vec4 v0000000002762850_0, 0, 4;
    %load/vec4 v0000000002762850_0;
    %parti/s 1, 3, 3;
    %cmpi/e 1, 0, 1;
    %jmp/0xz  T_2.0, 4;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0000000002762850_0, 0;
T_2.0 ;
    %jmp T_2;
    .thread T_2;
    .scope S_000000000279b3a0;
T_3 ;
    %wait E_0000000002799040;
    %delay 5000, 0;
    %vpi_call 2 21 "$display", "count = %d, out = %d", v0000000002762850_0, v00000000027628f0_0 {0 0 0};
    %jmp T_3;
    .thread T_3;
    .scope S_000000000279b3a0;
T_4 ;
    %vpi_call 2 25 "$dumpfile", "lab6.fsdb" {0 0 0};
    %vpi_call 2 26 "$dumpvars" {0 0 0};
    %pushi/vec4 0, 0, 1;
